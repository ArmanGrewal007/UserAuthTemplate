<% if flash[:notice] %> <!-- Doesnt have = coz we dont want to return value in html -->
    <div class="alert alert-info mt-4" role="alert"> <!-- alert, blue, margin-top 4 -->
        <%= flash[:notice] %>
    </div>
<%end%>

<% if flash[:alert] %> <!-- Doesnt have = coz we dont want to return value in html -->
    <div class="alert alert-warning mt-4" role="alert">
        <%= flash[:alert] %>
    </div>
<%end%>
